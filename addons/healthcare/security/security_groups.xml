<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    
        <record id="module_category_healthcare" model="ir.module.category">
            <field name="name">Healthcare</field>
            <field name="description">Access groups for Healthcare module</field>
            <field name="sequence">10</field>
        </record>

        <record id="group_mch_user" model="res.groups">
            <field name="name">MCH User</field>
            <field name="category_id" ref="module_category_healthcare"/>
        </record>

        <record id="group_mch_manager" model="res.groups">
            <field name="name">MCH Manager</field>
            <field name="category_id" ref="module_category_healthcare"/>
            <field name="implied_ids" eval="[(4, ref('group_mch_user'))]"/>
        </record>

        <record id="group_mch_provider" model="res.groups">
            <field name="name">Healthcare Provider</field>
            <field name="category_id" ref="module_category_healthcare"/>
            <field name="implied_ids" eval="[(4, ref('group_mch_user'))]"/>
        </record>

        <record id="group_mch_nurse" model="res.groups">
            <field name="name">Nurse</field>
            <field name="category_id" ref="module_category_healthcare"/>
            <field name="implied_ids" eval="[(4, ref('group_mch_user'))]"/>
        </record>

    </data>
</odoo>
